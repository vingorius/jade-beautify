{
  "name": "jade-beautify",
  "main": "./lib/jade-beautify",
  "version": "0.1.3",
  "description": "Simple Jade Beautify",
  "keywords": [
    "jade",
    "beautify",
    "formatter"
  ],
  "activationCommands": {
    "atom-workspace": [
      "jade-beautify:convert",
      "core:save"
    ],
    ".tree-view .file .name": [
      "jade-beautify:beautifyFile"
    ]
  },
  "repository": "https://github.com/vingorius/jade-beautify.git",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "pug-beautify": ">=0.0.2"
  }
}
